<configuration>
	<appSettings>
		<!-- The amount of time for the cache to expire, in minutes -->
		<add key="CacheExpiryTime" value="60" />
		<add key="DataFetchingStrategy" value="OpenExchangeRates" />
		<add key="OpenExchangeRates.ApiKey" value="" />
	</appSettings>
	<system.web>
		<compilation debug="true" targetFramework="4.5">
			<assemblies>
			</assemblies>
		</compilation>
		<httpRuntime targetFramework="4.5" />
	</system.web>
	<system.net>
		<requestCaching isPrivateCache="false" defaultPolicyLevel="CacheIfAvailable" />
	</system.net>
</configuration>